</div>\r\n        <div>Round: {{  }}</div>\r\n        <div>\r\n          <button /> Strict mode\r\n    \r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nexport default {\r\n    data: {      \r\n        count: 0,\r\n        possibilities: ['#green','#blue', '#red', '#dark'],\r\n        currentGame: [],\r\n        player: [],\r\n        strict: false\r\n    },\r\n    method: {\r\n        newGame() { \r\n        clearGame(); \r\n    },\r\n\r\n    clearGame() { \r\n    game.currentGame = []; \r\n    game.count = 0; \r\n    addCount(); \r\n    },\r\n\r\n    addCount() { \r\n        game.count ++; \r\n        $('#clickNumber').addClass('animated fadeOutDown'); \r\n        \r\n        setTimeout(function() { \r\n            $('# clickNumber').removeClass('fadeOutDown').html(game.count).addClass('fadeInDown'); \r\n        }, 200); \r\n    \r\n    generateMove(); \r\n        }\r\n    },\r\n    generateMove(){\r\n    game.currentGame.push(game.possibilities[(Math.floor(Math.random()*4))]);\r\n  showMoves();\r\n},\r\nshowMoves() {\r\n  var i = 0;\r\n  var moves = setInterval(function(){\r\n    playGame(game.currentGame[i]);\r\n    i++;\r\n    if (i >= game.currentGame.length) {\r\n      clearInterval(moves);\r\n    }\r\n  }, 600)\r\n  \r\n  clearPlayer();\r\n},\r\n\r\nclearPlayer() {\r\n  game.player = [];\r\n},\r\n\r\n addToPlayer(id) {\r\n  var field = \"#\"+id\r\n  console.log(field);\r\n  game.player.push(field);\r\n  playerTurn(field);\r\n},\r\n\r\nplayerTurn(x) {\r\n  if (game.player[game.player.length - 1] !== game.currentGame[game.player.length - 1]) {\r\n    if(game.strict){\r\n      alert('Try again! ...From scratch!');\r\n      newGame();\r\n    } else {\r\n      alert('Wrong move! Try again!');\r\n      showMoves();\r\n    }\r\n   } else {\r\n      console.log('Good Move!');\r\n      sound(x);\r\n      var check = game.player.length === game.currentGame.length;\r\n      if (check) {\r\n        if(game.count == 20){\r\n          alert('You won! Congrats.');\r\n        } else {\r\n          alert('Next round!');\r\n          nextLevel();\r\n        }\r\n      }\r\n    }\r\n},\r\n\r\nplayGame(field) {\r\n  $(field).addClass('hover');\r\n  sound(field);\r\n  setTimeout(function(){\r\n      $(field).removeClass('hover');\r\n  }, 300);\r\n}\r\n\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    .panel {\r\n        width: 150px;\r\n        height: 150px;\r\n        display: inline-block;\r\n        cursor: pointer;\r\n    }\r\n    .panel:hover {\r\n        border: black solid 1px;\r\n    }\r\n    .active:hover  {\r\n        background-color: #fff;\r\n    }\r\n\r\n    .top-left-pad {\r\n        border-top-left-radius: 180px;\r\n        background-color: red;\r\n    }\r\n    .top-right-pad {\r\n        border-top-right-radius: 180px;\r\n        background-color: blue;\r\n    }\r\n    .bottom-left-pad {\r\n        border-bottom-left-radius: 180px;\r\n        background-color: yellow;\r\n        \r\n    }\r\n    .bottom-right-pad {\r\n        border-bottom-right-radius: 180px;\r\n        background-color: green;\r\n        \r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}